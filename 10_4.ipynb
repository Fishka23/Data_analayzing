   "outputs": [],
   "source": [
    "data = {'Info': ['Name: Maggy, Age: 33', 'Name: Mike, Age: 23', 'Name: Bob, Age: 65']}"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
